    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.3.1",
    "vite": "^5.2.9",
    "vite-bundle-analyzer": "^0.9.2",
    "vitest": "^1.5.0",
    "vitest-canvas-mock": "^0.3.3"
  }
